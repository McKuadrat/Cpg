-- Scripts:

local function YXHZC_fake_script() -- MCADMIN.LocalScript 
	local script = Instance.new('LocalScript', MCADMIN)

	local Frame = script.Parent.Frame
	local player = game.Players.LocalPlayer
	local music = Instance.new("Sound")
	local yoga = false
	local animTrack = nil
	local animationTrack = nil
	local animationtarck = nil
	local box = Frame.Box.Text
	local Camera = workspace.CurrentCamera
	local last = {""}
	local baris = 1
	local UIS = game:GetService("UserInputService")
	music.Parent = player.Character.Head
	local scrollingFrame = Frame.ScrollingFrame -- misalnya LocalScript di dalam ScrollingFrame
	local listLayout = scrollingFrame:WaitForChild("UIListLayout")
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	local noclip = false
	local hari = os.date("*t")
	
	-- Update canvas size setiap kali konten berubah
	local function updateCanvas()
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y)
	end
	
	-- Pas awal
	updateCanvas()
	
	-- Saat ada perubahan ukuran (nambah/hapus elemen)
	listLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateCanvas)
	
	if hari.day == 5 and hari.month == 5 then
		Frame.TextLabel.Text = "🎁🎊MC ADMIN🎁🎊"
	elseif hari.day == 1 and hari.month == 1 then
		Frame.TextLabel.Text = "🎉MC ADMIN🎉"
	elseif hari.day == 25 and hari.month == 12 then
		Frame.TextLabel.Text = "🎄MC ADMIN🎄"
	elseif hari.day == 19 and hari.month == 3 and hari.year == 2026 then
		Frame.TextLabel.Text = "✨MC ADMIN✨"
	else
		Frame.TextLabel.Text = "MC ADMIN"
	end
	
	local function tambahPerintah(perintah)
		-- Cek apakah perintah sudah ada di dalam tabel
		for i = 1, #last do
			if last[i] == Frame.Box.Text then
				-- Jika sudah ada, pindahkan perintah ke depan (ke indeks pertama)
				table.remove(last, i)
				table.insert(last, 1, Frame.Box.Text)
				return
			end
		end
	
		-- Jika perintah belum ada, tambahkan perintah baru di depan tabel
		table.insert(last, 1, Frame.Box.Text)
		Frame.Box.Text = ""
		baris = 1  -- Reset baris ke perintah pertama setelah menambahkan perintah baru
	end
	
	Frame.Box.FocusLost:Connect(function()
		local args = string.split(Frame.Box.Text, " ")
		local cmd = args[1]:lower()
		
		if cmd == "tp" and #args == 4 then
			local x = tonumber(args[2])
			local y = tonumber(args[3])
			local z = tonumber(args[4])
			
			if x and y and z then
				player.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(x, y, z)
			end
			
			tambahPerintah(Frame.Box.Text)
			Frame.Box.Text = ""
		elseif cmd == "time" and #args >= 2 then
			game.Lighting.TimeOfDay = args[2]..":"..args[3]..":"..args[4]
			tambahPerintah(Frame.Box.Text)
			Frame.Box.Text = ""
		elseif cmd == "music" and #args >= 2 then
			local iden = args[2]:lower()
			local id = args[3]
			local volume = args[4]
			
			if iden then
				if id then
					music.SoundId = "rbxassetid://"..id
				end
				Frame.Box.Text = ""
				if volume then
					music.Volume = tonumber(volume)/10
				end
				if iden == "play" then
					music:Play()
				elseif iden == "stop" then
					music:Stop()
				end
				tambahPerintah(Frame.Box.Text)
				Frame.Box.Text = ""
			end
		elseif cmd == "nofog" then
			game.Lighting.FogStart = 0
			game.Lighting.FogEnd = 9999999
			tambahPerintah(Frame.Box.Text)
			Frame.Box.Text = ""
		elseif cmd == "goto" and #args == 2 then
			local name = args[2]
			
			if name then
				local target = game.Players:FindFirstChild(name)
				if target then
					player.Character:FindFirstChild("HumanoidRootPart").CFrame = target.Character:FindFirstChild("HumanoidRootPart").CFrame
					tambahPerintah(Frame.Box.Text)
					Frame.Box.Text = ""
				end
			end
		elseif cmd == "yoga" then
			local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
			local animm = Instance.new("Animation")
			animm.AnimationId = "rbxassetid://12507085924"  -- Ganti dengan link ID animasi
	
			if humanoid then
				yoga = not yoga
				if yoga then
					if not animTrack then
						-- Load dan simpan animasi jika belum ada
						animTrack = humanoid:LoadAnimation(animm)
					end
					animTrack:Play()  -- Mainkan animasi
				else
					if animTrack then
						animTrack:Stop()  -- Hentikan animasi jika sudah ada anim track
					end
				end
			end
			tambahPerintah(Frame.Box.Text)
			Frame.Box.Text = ""
		elseif cmd == "ban" and #args == 2 then
			local target = game.Workspace:FindFirstChild(args[2])
			local pt = game.Players:FindFirstChild(args[2])
			local ws = player.Character.Humanoid.WalkSpeed
			if target ~= player.Name then
				local anim = Instance.new("Animation")
				anim.AnimationId = "http://www.roblox.com/asset/?id=10714347256"
	
				if target then
					local humanoid = player.Character:FindFirstChild("Humanoid")
					local targeth = target:FindFirstChild("Humanoid")
					if humanoid and targeth then
						-- Memuat animasi satu kali
						local animTrack = humanoid:LoadAnimation(anim)
						animTrack:Play()
						player.Character.Humanoid.WalkSpeed = 0
						wait(2)
						local baneffect = Instance.new("Part")
						baneffect.Shape = "Cylinder"
						baneffect.Size = Vector3.new(159.279, 11.931, 30.782)
						baneffect.Color = Color3.new(0.227451, 0.490196, 0.0823529)
						baneffect.Transparency = 0.2
						baneffect.CFrame = target:FindFirstChild("HumanoidRootPart").CFrame
						baneffect.Anchored = true
						baneffect.CanCollide = false
						baneffect.Orientation = Vector3.new(0, 0, -90)
						baneffect.Material = Enum.Material.Plastic
						baneffect.Parent = target
						wait(2)
						target:Destroy()
						pt:Destroy()
						local sound = Instance.new("Sound")
						sound.SoundId = "rbxassetid://2674547670"
						sound.Parent = player
						sound:Play()
						-- Tunggu selama durasi animasi (misalnya 8 detik)
						wait(3)
						sound:Destroy()
						animTrack:Stop()
						player.Character.Humanoid.WalkSpeed = ws
						tambahPerintah(Frame.Box.Text)
					else
						local animTrack = humanoid:LoadAnimation(anim)
						animTrack:Play()
						player.Character.Humanoid.WalkSpeed = 0
						wait(7)
						game.StarterGui:SetCore("SendNotification", {
							Title = "MC ADMIN";
							Text = "Failed To Ban!";
							Duration = 5;
						})
						player.Character.Humanoid.WalkSpeed = ws
						animTrack:Stop()
					end
				end
			end
		elseif cmd == "day" then
			game.Lighting.TimeOfDay = "12:00:00"
			tambahPerintah(box)
		elseif cmd == "night" then
			game.Lighting.TimeOfDay = "18:00:00"
			tambahPerintah(box)
		elseif cmd == "speed" and #args == 2 then
			local value = tonumber(args[2])
			
			if value then
				player.Character.Humanoid.WalkSpeed = value
			end
			tambahPerintah()
		elseif cmd == "noclip" then
			noclip = not noclip
			if noclip then
				-- Menghentikan kolisi karakter
				for _, part in pairs(character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
			else
				-- Mengaktifkan kolisi karakter kembali
				for _, part in pairs(character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = true
					end
				end
			end
		tambahPerintah()
		elseif cmd == "backflip" then
			local anim = Instance.new("Animation")
			anim.AnimationId = "http://www.roblox.com/asset/?id=15693621070"
			local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
			
			if humanoid then
				yoga = not yoga
				if yoga then
					if not animationtarck then
						-- Load dan simpan animasi jika belum ada
						animationtarck = humanoid:LoadAnimation(anim)
					end
					animationtarck:Play()  -- Mainkan animasi
				else
					if animationtarck then
						animationtarck:Stop()  -- Hentikan animasi jika sudah ada anim track
					end
				end
			end
			tambahPerintah()
		elseif cmd == "f3x" then
			tambahPerintah()
			loadstring(game:HttpGet(('https://pastebin.com/raw/hp08JgV1'),true))()
		elseif cmd == "jump" and #args == 2 then
			local value = tonumber(args[2])
			
			if value then
				player.Character.Humanoid.JumpPower = value
			end
			tambahPerintah()
		elseif cmd == "gravity" or cmd == "grav" and #args == 2 then
			local value = tonumber(args[2])
			
			if value then
				game.Workspace.Gravity = value
				tambahPerintah()
			end
		elseif cmd == "godmode" then
			player.Character.Humanoid.MaxHealth = 9999999
			player.Character.Humanoid.Health = 9999999
			tambahPerintah()
		elseif cmd == "forcecfield" or cmd == "ff" then
			if player.Character:FindFirstChildWhichIsA("ForceField") then
				player.Character.ForceField:Destroy()
			else
				local ff = Instance.new("ForceField")
				ff.Parent = player.Character
			end
		elseif cmd == "view" and #args == 2 then
			local target = game:GetService("Players"):FindFirstChild(args[2])
			if target and target.Character and target.Character:FindFirstChild("Humanoid") then
				Camera.CameraSubject = target.Character:FindFirstChild("Humanoid")
				print("Now viewing:", args[2])
			else
				warn("Player tidak ditemukan atau belum spawn.")
			end
		elseif cmd == "unview" then
			Camera.CameraSubject = player.Character:FindFirstChild("Humanoid")
		end
	end)
	
	UIS.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end  -- Jika input sedang diproses oleh UI lainnya, lewati
	
		-- Cek jika tombol yang ditekan adalah panah atas
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Up then
			local panjang = #last  -- Panjang tabel last
	
			-- Jika baris kurang dari panjang tabel, tampilkan perintah sebelumnya
			if baris <= panjang and baris > 0 then
				Frame.Box.Text = last[baris]  -- Tampilkan perintah pada baris yang sesuai
				baris = baris + 1  -- Pindah ke perintah sebelumnya
			elseif baris > panjang then
				baris = 1  -- Kembali ke perintah pertama jika sudah di akhir
			end
		end
	end)
	
	player.Character.Animate.dance.Animation1.AnimationId = "http://www.roblox.com/asset/?id=10714340543"
	player.Character.Animate.dance.Animation2.AnimationId = "http://www.roblox.com/asset/?id=10714340543"
	player.Character.Animate.dance.Animation3.AnimationId = "http://www.roblox.com/asset/?id=10714340543"
end
coroutine.wrap(YXHZC_fake_script)()
