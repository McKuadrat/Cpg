-- Scripts:

local function CLWLFI_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local imagelabel = script.Parent
	imagelabel.Image = Players:GetUserThumbnailAsync(player.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
end
coroutine.wrap(CLWLFI_fake_script)()
local function PMLVUG_fake_script() -- nem.LocalScript 
	local script = Instance.new('LocalScript', nem)

	script.Parent.Text = "Name : "..game.Players.LocalPlayer.Name
end
coroutine.wrap(PMLVUG_fake_script)()
local function AATIT_fake_script() -- id.LocalScript 
	local script = Instance.new('LocalScript', id)

	script.Parent.Text = "ID : "..game.Players.LocalPlayer.UserId
end
coroutine.wrap(AATIT_fake_script)()
local function MRHTW_fake_script() -- hmm.MainScript 
	local script = Instance.new('LocalScript', hmm)

	local UserInputService = game:GetService("UserInputService")
	local pid = 0
	local rbx = "rbxassetid://"
	local premium = false
	
	script.Parent.Parent.premino:GetPropertyChangedSignal("Text"):Connect(function()
		if script.Parent.Parent.premino.Text == "Yes" then
			premium = true
		else
			premium = false
		end
	end)
	
	local function changemode()
		if pid == 0 then
			script.Parent.Frame.TextBox.PlaceholderText = "Speed"
			script.Parent.give.Visible = false
			script.Parent.change.Visible = true
		elseif pid == 1 then
			script.Parent.Frame.TextBox.PlaceholderText = "Jump"
		elseif pid == 2 then
			script.Parent.Frame.TextBox.PlaceholderText = "Health"
		elseif pid == 3 then
			script.Parent.Frame.TextBox.PlaceholderText = "Gravity"
		elseif pid == 4 then
			script.Parent.Frame.TextBox.PlaceholderText = "Time"
		elseif pid == 5 then
			script.Parent.Frame.TextBox.PlaceholderText = "Teleport"
			script.Parent.give.Visible = false
			script.Parent.change.Visible = true
		elseif pid == 6 then
			script.Parent.give.Visible = true
			script.Parent.change.Visible = false
			script.Parent.Frame.Visible = false
		elseif pid == 7 then
			script.Parent.give.Visible = true
			script.Parent.change.Visible = false
			script.Parent.Frame.Visible = false
		end
	end
	
	local function abilityedit()
		if pid == 0 then
			pid = 1
			script.Parent.Image = rbx..6684647765
		elseif pid == 1 then
			pid = 2
			script.Parent.Image = rbx..119725594058749
		elseif pid == 2 and premium == true then
			pid = 3
			script.Parent.Image = "rbxassetid://11930634139"
		elseif pid == 3 then
			pid = 4
			script.Parent.Image = rbx..12798038904
		elseif pid == 4 then
			pid = 5
			script.Parent.Image = rbx..16773466765
		elseif pid == 5 then
			pid = 6
			script.Parent.Image = rbx..66657338
		elseif pid == 6 then
			pid = 7
			script.Parent.Image = rbx..18600008546
		elseif pid == 7 then
			pid = 0
			script.Parent.Image = "rbxassetid://98392185727723"
		elseif pid == 2 and premium == false then
			pid = 0
			script.Parent.Image = "rbxassetid://98392185727723"
		end
	end
	
	local function ffor()
		local ff = Instance.new("ForceField")
		ff.Parent = game.Players.LocalPlayer.Character
	end
	
	-- Fungsi untuk mendeteksi ketika pemain menekan tombol
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		-- Cek apakah input adalah key press dan bukan event dari game
		if not gameProcessed then
			-- Cek apakah tombol yang ditekan adalah ']'
			if input.KeyCode == Enum.KeyCode.RightBracket and pid == 0 then
				pid = 1
				script.Parent.Image = rbx..6684647765
			elseif input.KeyCode == Enum.KeyCode.RightBracket and pid == 1 then
				pid = 2
				script.Parent.Image = rbx..119725594058749
			elseif input.KeyCode == Enum.KeyCode.RightBracket and pid == 2 and premium == true then
				pid = 3
				script.Parent.Image = "rbxassetid://11930634139"
			elseif input.KeyCode == Enum.KeyCode.RightBracket and pid == 3 and premium == true then
				pid = 4
				script.Parent.Image = rbx..12798038904
			elseif input.KeyCode == Enum.KeyCode.RightBracket and pid == 4 and premium == true then
				pid = 5
				script.Parent.Image = rbx..16773466765
			elseif input.KeyCode == Enum.KeyCode.RightBracket and pid == 5 and premium == true then
				pid = 6
				script.Parent.Image = rbx..66657338
			elseif input.KeyCode == Enum.KeyCode.RightBracket and pid == 6 and premium == true then
				pid = 7
				script.Parent.Image = rbx..18600008546
			end
			
			if input.KeyCode == Enum.KeyCode.LeftBracket and pid == 1 then
				pid = 0
				script.Parent.Image = "rbxassetid://98392185727723"
			elseif input.KeyCode == Enum.KeyCode.LeftBracket and pid == 2 then
				pid = 1
				script.Parent.Image = rbx..6684647765
			elseif input.KeyCode == Enum.KeyCode.LeftBracket and pid == 3 and premium == true then
				pid = 2
				script.Parent.Image = rbx..119725594058749
			elseif input.KeyCode == Enum.KeyCode.LeftBracket and pid == 4 and premium == true then
				pid = 3
				script.Parent.Image = "rbxassetid://11930634139"
			elseif input.KeyCode == Enum.KeyCode.LeftBracket and pid == 5 and premium == true then
				pid = 4
				script.Parent.Image = rbx..12798038904
			elseif input.KeyCode == Enum.KeyCode.LeftBracket and pid == 6 and premium == true then
				pid = 5
				script.Parent.Image = rbx..16773466765
			elseif input.KeyCode == Enum.KeyCode.LeftBracket and pid == 7 and premium == true then
				pid = 6
				script.Parent.Image = rbx..66657338
			end
			
			-- open
			if input.KeyCode == Enum.KeyCode.E and script.Parent.change.Visible == true then
				script.Parent.Frame.Visible = not script.Parent.Frame.Visible
				changemode()
			elseif input.KeyCode == Enum.KeyCode.E and script.Parent.give.Visible == true and pid == 6 then
				ffor()
			elseif input.KeyCode == Enum.KeyCode.E and script.Parent.give.Visible == true and pid == 7 then
				loadstring(game:GetObjects("rbxassetid://6695644299")[1].Source)()
			end
		end
	end)
	
	script.Parent.ImageButton.MouseButton1Up:Connect(abilityedit)
	
	script.Parent.change.MouseButton1Up:Connect(function()
		script.Parent.Frame.Visible = not script.Parent.Frame.Visible
		changemode()
	end)
	
	script.Parent.give.MouseButton1Up:Connect(ffor)
	
	script.Parent:GetPropertyChangedSignal("Image"):Connect(changemode)
end
coroutine.wrap(MRHTW_fake_script)()
local function ZQRA_fake_script() -- Frame.DraggableGUI 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(ZQRA_fake_script)()
local function PXZJP_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local j = script.Parent
	
	script.Parent.TextButton.MouseButton1Up:Connect(function()
		if j.TextBox.PlaceholderText == "Speed" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(j.TextBox.Text)
		elseif j.TextBox.PlaceholderText == "Jump" then
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = tonumber(j.TextBox.Text)
		elseif j.TextBox.PlaceholderText == "Health" then
			game.Players.LocalPlayer.Character.Humanoid.MaxHealth = tonumber(j.TextBox.Text)
			game.Players.LocalPlayer.Character.Humanoid.Health = tonumber(j.TextBox.Text)
		elseif j.TextBox.PlaceholderText == "Gravity" then
			game.Workspace.Gravity = tonumber(j.TextBox.Text)
		elseif j.TextBox.PlaceholderText == "Time" then
			game.Lighting.TimeOfDay = tonumber(j.TextBox.Text)
		elseif j.TextBox.PlaceholderText == "Teleport" then
			local args = string.split(j.TextBox.Text, " ")
			
			if #args == 3 then
				game.Players.LocalPlayer.Character:MoveTo(Vector3.new(args[1], args[2], args[3]))
			end
		end
	end)
	
	
end
coroutine.wrap(PXZJP_fake_script)()
