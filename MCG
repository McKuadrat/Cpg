local function AVQQA_fake_script() -- passkey.LocalScript 
	local script = Instance.new('LocalScript', passkey)

	if game.Players.LocalPlayer.Name == "Fizss9" or game.Players.LocalPlayer.Name == "gwindoaslibeneran" then
		script.Parent.Parent.Stats.premino.Text = "Yes"
		script.Parent.Parent.Stats.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "MC Trainer",
			Text = "Skipping Key",
			Duration = 5,
			Button1 = "OK"
		})
		script.Parent:Destroy()
	end
	
	script.Parent.getkey.MouseButton1Up:Connect(function()
		setclipboard(script.Parent.getkeylink.Text)
	end)
	
	script.Parent.enter.MouseButton1Up:Connect(function()
		if script.Parent.TextBox.Text == "737kd8G02JD87123" then
			script.Parent.Parent.Stats.premino.Text = "Yes"
			script.Parent.Parent.Stats.Visible = true
			game.StarterGui:SetCore("SendNotification", {
				Title = "MC Trainer",
				Text = "Key Correct",
				Duration = 5,
				Button1 = "OK"
			})
			script.Parent:Destroy()
		end
	end)
	
	script.Parent.trial.MouseButton1Up:Connect(function()
		script.Parent.Parent.Stats.premino.Text = "No"
		script.Parent.Parent.Stats.Visible = true
		game.StarterGui:SetCore("SendNotification", {
			Title = "MC Trainer",
			Text = "Trial Mode",
			Duration = 5,
			Button1 = "OK"
		})
		script.Parent:Destroy()
	end)
end
coroutine.wrap(AVQQA_fake_script)()
