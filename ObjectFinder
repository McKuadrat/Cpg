-- Scripts:

local function ZZAILIN_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local player = game.Players.LocalPlayer
	local tikus = player:GetMouse()
	local click = true
	local infor = script.Parent.Parent.information
	local selecti = Instance.new("SelectionBox")
	selecti.Name = "MCKBOXSELECT"
	local testt = Instance.new("LocalScript")
	local target = nil
	local touch = false
	local anak = infor.Parent.anak
	local group = anak.Parent.Groups
	local adaanak = false
	local UIS = game:GetService("UserInputService")
	
	local function updateCanvas(scrollingFrame)
		local listLayout = scrollingFrame:WaitForChild("UIListLayout")
		
		scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y)
	end
	
	local function updateCanvasSize(textLabel, scrollingFrame)
		-- Ukur ukuran aktual teks
		local textSize = textLabel.TextBounds
	
		-- Ambil ukuran tinggi dari ScrollingFrame (supaya Y gak berubah)
		local currentY = scrollingFrame.CanvasSize.Y
	
		-- Tambahkan sedikit padding opsional kalau mau
		local padding = 40
	
		-- Set CanvasSize X agar sesuai panjang teks
		scrollingFrame.CanvasSize = UDim2.new(0, textSize.X + padding, currentY.Scale, currentY.Offset)
		textLabel.Size = UDim2.new(0, textSize.X + 500, textLabel.Size.Y.Scale, textLabel.Size.Y.Offset)
	end
	
	local function GetPath(obj)
		local path = obj.Name
		local current = obj
	
		while current.Parent and current.Parent ~= game do
			current = current.Parent
			path = current.Name .. "." .. path
		end
	
		-- Tambahkan 'game' di awal
		if current.Parent == game then
			path = "game." .. path
		end
	
		return path
	end
	
	UIS.InputBegan:Connect(function(a, b)
		if b then return end
		
		if a.KeyCode == Enum.KeyCode.P then
			touch = not touch
			if touch == false then
				script.Parent.Touch.BackgroundColor3 = Color3.new(0, 0, 0)
			else
				script.Parent.Touch.BackgroundColor3 = Color3.new(1, 0.584314, 0)
			end
		end
	end)
	
	tikus.Button1Down:Connect(function()
		local part = tikus.Target
		if part and part:IsA("BasePart") and touch == true then
			target = part.Name
			selecti.Adornee = part
			selecti.Parent = game.Workspace
			selecti.Color3 = Color3.new(0.133333, 1, 0)
			
			script.Parent.TextBox.Text = part.Name
			
			infor.Frame.name.Text = "Name : " .. part.Name
			infor.Frame.cn.Text = "ClassName : " .. part.ClassName
			
			local pos = part.Position
			local flooring = Vector3.new(
				math.floor(pos.X),
				math.floor(pos.Y),
				math.floor(pos.Z)
			)
			
			if part.Material then
				infor.Frame.mate.Text = "Material : " .. part.Material.Name
			else
				infor.Frame.mate.Text = "Material : nil"
			end
	
			if part.Position then
				infor.Frame.pos.Text = "Position : " .. tostring(flooring)
			else
				infor.Frame.pos.Text = "Position : nil"
			end
	
			-- Cek apakah ada Script atau LocalScript di dalam part
			local hasScript = false
			for _, obj in pairs(part:GetDescendants()) do
				if obj:IsA("Script") or obj:IsA("LocalScript") then
					hasScript = true
					break
				end
			end
	
			if hasScript then
				infor.Frame.hasc.Text = "Script : Yes"
			else
				infor.Frame.hasc.Text = "Script : No"
			end
			
			local path = GetPath(part)
			
			infor.Frame.ScrollingFrame.Location.Text = "Path : "..path
			
			for _, a in pairs(part:GetChildren()) do
				if a:IsA("BasePart") then
					adaanak = true
					break
				end
			end
			
			if adaanak then
				infor.Frame.haschild.Text = "Has Child : Yes"
				infor.Frame.Frame.gc.Visible = true
				adaanak = false
			else
				infor.Frame.haschild.Text = "Has Child : No"
				infor.Frame.Frame.gc.Visible = false
				adaanak = false
			end
			
			if part.Parent:IsA("Folder") or part.Parent.ClassName == "Model" then
				infor.Frame.Frame.gg.Visible = true
				infor.Frame.ig.Text = "In Groups : Yes"
			else
				infor.Frame.Frame.gg.Visible = false
				infor.Frame.ig.Text = "In Groups : No"
			end
			
			updateCanvasSize(infor.Frame.ScrollingFrame.Location, infor.Frame.ScrollingFrame)
		elseif touch == true then
			selecti.Adornee = nil
			selecti.Parent = nil
			script.Parent.TextBox.Text = ""
			target = nil
			infor.Visible = false
		end
	end)
	
	
	script.Parent.Information.MouseButton1Up:Connect(function()
		if target == nil then
			game.StarterGui:SetCore("SendNotification", {
				Title = "Notification",
				Text = "Please Select A Part First",
				Duration = 5
			})
		else
			infor.Visible = not infor.Visible
			anak.Visible = false
			group.Visible = false
		end
	end)
	
	script.Parent.search.MouseButton1Up:Connect(function()
		for _, part in pairs(game:GetDescendants()) do
			if part and part:IsA("BasePart") and part.Name == script.Parent.TextBox.Text and part:IsDescendantOf(game.Workspace) then
				game.StarterGui:SetCore("SendNotification", {
					Title = "Part Found",
					Text = "Part Was Found!, You Can Check In Information!",
					Duration = 5
				})
				target = part.Name
			selecti.Adornee = part
			selecti.Parent = game.Workspace
			selecti.Color3 = Color3.new(0.133333, 1, 0)
			
			script.Parent.TextBox.Text = part.Name
			
			infor.Frame.name.Text = "Name : " .. part.Name
			infor.Frame.cn.Text = "ClassName : " .. part.ClassName
			
			local pos = part.Position
			local flooring = Vector3.new(
				math.floor(pos.X),
				math.floor(pos.Y),
				math.floor(pos.Z)
			)
			
			if part.Material then
				infor.Frame.mate.Text = "Material : " .. part.Material.Name
			else
				infor.Frame.mate.Text = "Material : nil"
			end
	
			if part.Position then
				infor.Frame.pos.Text = "Position : " .. tostring(flooring)
			else
				infor.Frame.pos.Text = "Position : nil"
			end
	
			-- Cek apakah ada Script atau LocalScript di dalam part
			local hasScript = false
			for _, obj in pairs(part:GetDescendants()) do
				if obj:IsA("Script") or obj:IsA("LocalScript") then
					hasScript = true
					break
				end
			end
	
			if hasScript then
				infor.Frame.hasc.Text = "Script : Yes"
			else
				infor.Frame.hasc.Text = "Script : No"
			end
			
			local path = GetPath(part)
			
			infor.Frame.ScrollingFrame.Location.Text = "Path : "..path
			
			updateCanvasSize(infor.Frame.ScrollingFrame.Location, infor.Frame.ScrollingFrame)
			
			for _, a in pairs(part:GetChildren()) do
				if a:IsA("BasePart") then
					adaanak = true
					break
				end
			end
			
			if adaanak then
				infor.Frame.haschild.Text = "Has Child : Yes"
				infor.Frame.Frame.gc.Visible = true
				adaanak = false
			else
				infor.Frame.haschild.Text = "Has Child : No"
				infor.Frame.Frame.gc.Visible = false
				adaanak = false
			end
			
				if part.Parent:IsA("Folder") or part.Parent.ClassName == "Model" then
					infor.Frame.Frame.gg.Visible = true
					infor.Frame.ig.Text = "In Groups : Yes"
				else
					infor.Frame.Frame.gg.Visible = false
					infor.Frame.ig.Text = "In Groups : No"
				end
			break
			else
				if part and part:IsA("BasePart") and part.Name == script.Parent.TextBox.Text then
					game.StarterGui:SetCore("SendNotification", {
						Title = "Part Found",
						Text = "Part Was Found!, You Can Check In Information!",
						Duration = 5
					})
					target = part.Name
	
					infor.Frame.name.Text = "Name : " .. part.Name
					infor.Frame.cn.Text = "ClassName : " .. part.ClassName
	
					if part.Material then
						infor.Frame.mate.Text = "Material : nil"
					else
						infor.Frame.mate.Text = "Material : nil"
					end
	
					if part.Position then
						infor.Frame.pos.Text = "Position : nil"
					else
						infor.Frame.pos.Text = "Position : nil"
					end
	
					-- Cek apakah ada Script atau LocalScript di dalam part
					local hasScript = false
					for _, obj in pairs(part:GetDescendants()) do
						if obj:IsA("Script") or obj:IsA("LocalScript") then
							hasScript = true
							break
						end
					end
	
					if hasScript then
						infor.Frame.hasc.Text = "Script : Yes"
					else
						infor.Frame.hasc.Text = "Script : No"
					end
	
					local path = GetPath(part)
	
					infor.Frame.ScrollingFrame.Location.Text = "Path : "..path
					
					for _, a in pairs(part:GetChildren()) do
						if a:IsA("BasePart") then
							adaanak = true
							break
						end
					end
	
					if adaanak then
						infor.Frame.haschild.Text = "Has Child : Yes"
						infor.Frame.Frame.gc.Visible = true
						adaanak = false
					else
						infor.Frame.haschild.Text = "Has Child : No"
						infor.Frame.Frame.gc.Visible = false
						adaanak = false
					end
					
					if part.Parent:IsA("Model") or part.Parent:IsA("Folder") then
						infor.Frame.Frame.gg.Visible = true
					else
						infor.Frame.Frame.gg.Visible = false
					end
	
					updateCanvasSize(infor.Frame.ScrollingFrame.Location, infor.Frame.ScrollingFrame)
					break
				end
			end
		end
	end)
	
	script.Parent.Touch.MouseButton1Up:Connect(function()
		if touch == false then
			touch = true
			script.Parent.Touch.BackgroundColor3 = Color3.new(1, 0.584314, 0)
		else
			touch = false
			script.Parent.Touch.BackgroundColor3 = Color3.new(0, 0, 0)
		end
	end)
	
	infor.Frame.Frame.gc.MouseButton1Up:Connect(function()
		anak.Visible = true
		infor.Visible = false
		anak.Position = infor.Position
		
		for _, anakd in pairs(anak.ScrollingFrame:GetChildren()) do
			if not anakd:IsA("UIListLayout") then
				anakd:Destroy()
			end
		end
		
		for _, part in pairs(selecti.Adornee:GetChildren()) do
			if part:IsA("BasePart") then
				local template = anak.template:Clone()
				template.Parent = anak.ScrollingFrame
				template.Name = part.Name
				template.Text = part.Name
				template.Visible = true
				template.MouseButton1Up:Connect(function()
					if part and part:IsA("BasePart") then
						game.StarterGui:SetCore("SendNotification", {
							Title = "Part Found",
							Text = "Part Was Found!, You Can Check In Information!",
							Duration = 5
						})
						target = part.Name
						selecti.Adornee = part
						selecti.Parent = game.Workspace
						selecti.Color3 = Color3.new(0.133333, 1, 0)
	
						script.Parent.TextBox.Text = part.Name
	
						infor.Frame.name.Text = "Name : " .. part.Name
						infor.Frame.cn.Text = "ClassName : " .. part.ClassName
	
						local pos = part.Position
						local flooring = Vector3.new(
							math.floor(pos.X),
							math.floor(pos.Y),
							math.floor(pos.Z)
						)
	
						if part.Material then
							infor.Frame.mate.Text = "Material : " .. part.Material.Name
						else
							infor.Frame.mate.Text = "Material : nil"
						end
	
						if part.Position then
							infor.Frame.pos.Text = "Position : " .. tostring(flooring)
						else
							infor.Frame.pos.Text = "Position : nil"
						end
	
						-- Cek apakah ada Script atau LocalScript di dalam part
						local hasScript = false
						for _, obj in pairs(part:GetDescendants()) do
							if obj:IsA("Script") or obj:IsA("LocalScript") then
								hasScript = true
								break
							end
						end
	
						if hasScript then
							infor.Frame.hasc.Text = "Script : Yes"
						else
							infor.Frame.hasc.Text = "Script : No"
						end
	
						local path = GetPath(part)
	
						infor.Frame.ScrollingFrame.Location.Text = "Path : "..path
	
						updateCanvasSize(infor.Frame.ScrollingFrame.Location, infor.Frame.ScrollingFrame)
	
						for _, a in pairs(part:GetChildren()) do
							if a:IsA("BasePart") then
								adaanak = true
								break
							end
						end
	
						if adaanak then
							infor.Frame.haschild.Text = "Has Child : Yes"
							infor.Frame.Frame.gc.Visible = true
							adaanak = false
						else
							infor.Frame.haschild.Text = "Has Child : No"
							infor.Frame.Frame.gc.Visible = false
							adaanak = false
						end
						
						if part.Parent:IsA("Folder") or part.Parent.ClassName == "Model" then
							infor.Frame.Frame.gg.Visible = true
							infor.Frame.ig.Text = "In Groups : Yes"
						else
							infor.Frame.Frame.gg.Visible = false
							infor.Frame.ig.Text = "In Groups : No"
						end
						
						anak.Visible = false
						infor.Visible = true
					end
				end)
			end
		end
		updateCanvas(anak.ScrollingFrame)
	end)
	
	infor.Frame.Frame.gg.MouseButton1Up:Connect(function()
		group.Visible = true
		infor.Visible = false
		
		for _, grop in pairs(group.ScrollingFrame:GetChildren()) do
			if not grop:IsA("UIListLayout") then
				grop:Destroy()
			end
		end
		
		for _, part in pairs(selecti.Adornee.Parent:GetChildren()) do
			if part:IsA("BasePart") then
				local template = group.template:Clone()
				template.Parent = group.ScrollingFrame
				template.Name = part.Name.."_"..tostring(_)
				template.Text = part.Name
				template.Visible = true
				template.MouseButton1Up:Connect(function()
					if part and part:IsA("BasePart") then
						game.StarterGui:SetCore("SendNotification", {
							Title = "Part Found",
							Text = "Part Was Found!, You Can Check In Information!",
							Duration = 5
						})
						target = part.Name
						selecti.Adornee = part
						selecti.Parent = game.Workspace
						selecti.Color3 = Color3.new(0.133333, 1, 0)
	
						script.Parent.TextBox.Text = part.Name
	
						infor.Frame.name.Text = "Name : " .. part.Name
						infor.Frame.cn.Text = "ClassName : " .. part.ClassName
	
						local pos = part.Position
						local flooring = Vector3.new(
							math.floor(pos.X),
							math.floor(pos.Y),
							math.floor(pos.Z)
						)
	
						if part.Material then
							infor.Frame.mate.Text = "Material : " .. part.Material.Name
						else
							infor.Frame.mate.Text = "Material : nil"
						end
	
						if part.Position then
							infor.Frame.pos.Text = "Position : " .. tostring(flooring)
						else
							infor.Frame.pos.Text = "Position : nil"
						end
	
						-- Cek apakah ada Script atau LocalScript di dalam part
						local hasScript = false
						for _, obj in pairs(part:GetDescendants()) do
							if obj:IsA("Script") or obj:IsA("LocalScript") then
								hasScript = true
								break
							end
						end
	
						if hasScript then
							infor.Frame.hasc.Text = "Script : Yes"
						else
							infor.Frame.hasc.Text = "Script : No"
						end
	
						local path = GetPath(part)
	
						infor.Frame.ScrollingFrame.Location.Text = "Path : "..path
	
						updateCanvasSize(infor.Frame.ScrollingFrame.Location, infor.Frame.ScrollingFrame)
	
						for _, a in pairs(part:GetChildren()) do
							if a:IsA("BasePart") then
								adaanak = true
								break
							end
						end
	
						if adaanak then
							infor.Frame.haschild.Text = "Has Child : Yes"
							infor.Frame.Frame.gc.Visible = true
							adaanak = false
						else
							infor.Frame.haschild.Text = "Has Child : No"
							infor.Frame.Frame.gc.Visible = false
							adaanak = false
						end
	
						if part.Parent:IsA("Folder") or part.Parent.ClassName == "Model" then
							infor.Frame.Frame.gg.Visible = true
							infor.Frame.ig.Text = "In Groups : Yes"
						else
							infor.Frame.Frame.gg.Visible = false
							infor.Frame.ig.Text = "In Groups : No"
						end
	
						group.Visible = false
						infor.Visible = true
					end
				end)
			end
		end
		updateCanvas(group.ScrollingFrame)
	end)
	
	infor.Parent.TextButton.MouseButton1Up:Connect(function()
		script.Parent.Visible = not script.Parent.Visible
	end)
end
coroutine.wrap(ZZAILIN_fake_script)()
local function CVHTVG_fake_script() -- TextButton.DraggableGUI 
	local script = Instance.new('LocalScript', TextButton)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(CVHTVG_fake_script)()
